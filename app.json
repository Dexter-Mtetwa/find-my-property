{
  "expo": {
    "name": "FindMyProperty",
    "slug": "findmyproperty",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "findmyproperty",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hushirama.findmyproperty",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#667eea"
      },
      "package": "com.hushirama.findmyproperty"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bd55c1cd-7798-4b5f-bf51-d64d5340c530"
      }
    }
  }
}
